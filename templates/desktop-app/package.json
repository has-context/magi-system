{
  "name": "electron-desktop-app",
  "productName": "Electron Desktop App",
  "version": "1.0.0",
  "description": "Cross-platform desktop application built with Electron",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "dev": "electron-forge start",
    "package": "electron-forge package",
    "package:all": "electron-forge package --platform=all",
    "make": "electron-forge make",
    "dist": "electron-forge make --targets=@electron-forge/maker-squirrel,@electron-forge/maker-zip,@electron-forge/maker-deb,@electron-forge/maker-dmg",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [
    "electron",
    "desktop",
    "react",
    "typescript"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "dependencies": {
    "electron-log": "^5.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "dotenv": "^16.4.0",
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/maker-deb": "^7.3.0",
    "@electron-forge/maker-dmg": "^7.3.0",
    "@electron-forge/maker-rpm": "^7.3.0",
    "@electron-forge/maker-squirrel": "^7.3.0",
    "@electron-forge/maker-zip": "^7.3.0",
    "@electron-forge/plugin-webpack": "^7.3.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.1",
    "electron": "^30.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "node-loader": "^2.0.0",
    "sass": "^1.74.0",
    "sass-loader": "^14.1.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.2"
  }
}

{
  "name": "magi-system",
  "version": "1.0.0",
  "description": "MAGI System",
  "main": "index.js",
  "scripts": {
    "build": "tsc && webpack && rsync -a --exclude=\"*.ts\" --exclude=\"*.js\" controller/client/ dist/controller/client/",
    "start": "node dist/controller/server/server.js",
    "cleanup": "rm -rf dist/",
    "dev": "npm run cleanup && nodemon",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "setup": "tsc setup/setup.ts --esModuleInterop && node dist/setup/setup.js",
    "setup-claude": "tsc setup/claude_setup.ts --esModuleInterop && node dist/setup/claude_setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/has-context/magi-system.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/has-context/magi-system/issues"
  },
  "homepage": "https://github.com/has-context/magi-system#readme",
  "dependencies": {
    "child_process": "^1.0.2",
    "dockerode": "^4.0.4",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "marked": "^15.0.7",
    "socket.io": "^4.8.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.35",
    "@types/express": "^5.0.0",
    "@types/marked": "^5.0.2",
    "@types/node": "^22.13.10",
    "@types/socket.io": "^3.0.1",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "nodemon": "^3.1.9",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
